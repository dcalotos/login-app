{
  "name": "login-app-e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for login application using WebDriverIO",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.ts",
    "test:local": "wdio run ./wdio.conf.ts",
    "test:browserstack": "wdio run ./wdio.browserstack.conf.ts",
    "test:mobile": "wdio run ./wdio.appium.conf.ts",
    "test:mobile:android": "wdio run ./wdio.appium.conf.ts --spec ./test/specs/mobile/**/*.ts --platform android",
    "test:mobile:ios": "wdio run ./wdio.appium.conf.ts --spec ./test/specs/mobile/**/*.ts --platform ios"
  },
  "keywords": [
    "webdriverio",
    "e2e",
    "testing",
    "appium",
    "browserstack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@wdio/allure-reporter": "^8.27.0",
    "@wdio/appium-service": "^8.27.0",
    "@wdio/browserstack-service": "^8.27.0",
    "@wdio/cli": "^8.27.0",
    "@wdio/local-runner": "^8.27.0",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/spec-reporter": "^8.27.0",
    "@wdio/types": "^8.27.0",
    "allure-commandline": "^2.25.0",
    "appium": "^2.2.0",
    "appium-uiautomator2-driver": "^2.34.0",
    "appium-xcuitest-driver": "^5.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "wdio-chromedriver-service": "^8.1.1"
  },
  "dependencies": {
    "webdriverio": "^8.27.0"
  }
}
